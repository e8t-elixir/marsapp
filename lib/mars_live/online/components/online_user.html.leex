
<div>
  <h2>Online: </h2>
  <%= for {user_id, user} <- @users do %>
    <%= if user_id == @current_user[:id] do %>
      <span class="px-2 py-1 text-lg bg-green-200 rounded"><%= user[:name] %> (me)</span>
    <% else %>
      <span class="px-2 py-1 text-base bg-blue-200 rounded"><%= user[:name] %></span>
    <% end %>
  <% end %>
</div>

